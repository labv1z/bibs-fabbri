@Article{Zucker88Size,
  author = 	{S. W. Zucker and Davis},
  title = 	{Size--Density Constraint and Dot Grouping},
  journal = 	{Perception},
  year = 	1988,
  OPTmonth = 	"",
  volume = 	17,
  OPTnumber = 	"",
  pages = 	{229--247},
  summary =	{Explore differences between dense and sparse dotted
		 contours. Argue there is a separation at a dot
		 spacing/diameter ratio of 5:1 between dense and
		 sparse. It is argued that dense side of this
		 constraint is processed like continuous (which
		 doesn't actually exist for discrete system). It is
		 argued that only densely dotted patterns have
		 distinguished points that act as endpoints (e.g. in
		 Kanizsa edge and sun illusion). Also use alternating
		 opposite contrast dots and find the density depends
		 on similar polarity dots. N.B.: The misconception of
		 small RF==tangent, large RF==curvature is promulgated
		 here, even though the Nature paper is cited. SWZ
		 still at his 1985 formulation.}
}

